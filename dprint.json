{
  "incremental": true,
  "typescript": {
    "semiColons": "always",
    "indentWidth": 2,
    "quoteStyle": "alwaysDouble",
    "spaceSurroundingProperties": true
  },
  "json": {},
  "markdown": {},
  "toml": {},
  "prettier": {
    "trailingComma": "all",
    "singleQuote": false,
    "bracketSpacing": true
  },
  "includes": ["**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml,yml,yaml}"],
  "excludes": ["**/node_modules", "**/*-lock.json"],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.57.2.wasm",
    "https://plugins.dprint.dev/json-0.13.0.wasm",
    "https://plugins.dprint.dev/markdown-0.10.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.2.wasm",
    "https://plugins.dprint.dev/prettier-0.2.2.exe-plugin@63b06beba3acd51e6d23379c47db342a1a24f3d88f3c52a775e2db3426124582"
  ]
}
